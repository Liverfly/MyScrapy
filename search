			    	$("input[name='o1']").each(function() {
			    		if($(this).val() == "null")
			    		{
			    			$(this).attr("checked",true)
			    		}});
				} else if (t == "blog") {
					$(".search-type")[0].children[2].style.color="#990000";
					
		    		$("#o_news")[0].style.display="none";
